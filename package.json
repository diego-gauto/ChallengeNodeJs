{
  "name": "node.js",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "build": "tsc",
    "dev": "cross-env NODE_ENV=development nodemon src/main.ts --exec ts-node",
    "prod": "cross-env NODE_ENV=production ts-node src/main.ts",
    "start": "node dist/main.js",
    "migration:generate": "ts-node ./node_modules/typeorm/cli.js migration:generate -d src/config/typeorm.ts ./migrations/auto-migration",
    "migration:run": "ts-node ./node_modules/typeorm/cli.js migration:run -d src/config/typeorm.ts",
    "migration:revert": "ts-node ./node_modules/typeorm/cli.js migration:revert -d src/config/typeorm.ts",
    "typeorm": "ts-node ./node_modules/typeorm/cli.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^16.11.6",
    "@types/node-cron": "^3.0.2",
    "@types/nodemailer": "^6.4.4",
    "@types/pg": "^8.6.5",
    "apollo-server-errors": "^3.3.1",
    "apollo-server-express": "^3.4.0",
    "bcryptjs": "^2.4.3",
    "class-validator": "^0.13.2",
    "dotenv": "^16.0.0",
    "express": "^4.17.1",
    "graphql-compose": "^9.0.8",
    "graphql-composer-decorators": "^1.0.23",
    "jsonwebtoken": "^8.5.1",
    "mysql": "^2.18.1",
    "node-cron": "^3.0.1",
    "nodemailer": "^6.7.7",
    "pg": "^8.7.3",
    "pino": "^8.0.0",
    "pino-pretty": "^8.1.0",
    "reflect-metadata": "^0.1.13",
    "type-graphql": "^1.1.1",
    "typescript": "^4.4.4",
    "typeorm": "^0.2.38"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "nodemon": "^2.0.14",
    "ts-node": "10.9.2"
  }
}
